					MOVE_UNIT LeonsterLanceKnight, [6, 24], [6, 19], 1, None
          			YIELD_UNK

					WAIT_MOVE
					YIELD

					macroASMCRemoveUnit LeonsterLanceKnight

					FADE_OUT 2
					YIELD

					SET_CAMERA_POSITION [33, 16]
					YIELD

					STORE_WORD wCurrentChapter, Chapter10
